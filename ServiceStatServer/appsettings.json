{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      
      "HttpsInlineCertStore": {
        "Url": "https://genserv:5001",
        "Certificate": {
          "Subject": "genserv",
          "Store": "My",
          "Location": "LocalMachine",
          "AllowInvalid": true
        }
      }
    },
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  }
}
